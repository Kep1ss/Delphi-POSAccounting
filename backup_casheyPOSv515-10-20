-- 
-- Table structure for table  m_user
-- 

DROP TABLE IF EXISTS m_user;
CREATE TABLE `m_user` (
  `iduser` int(11) NOT NULL AUTO_INCREMENT,
  `username` varchar(15) DEFAULT NULL,
  `namalengkap` varchar(50) DEFAULT NULL,
  `level` varchar(25) DEFAULT NULL,
  `pwd` varchar(255) DEFAULT NULL,
  `showharga` tinyint(1) DEFAULT '0',
  `editharga` tinyint(1) DEFAULT '0',
  PRIMARY KEY (`iduser`),
  UNIQUE KEY `username` (`username`)
) ENGINE=InnoDB AUTO_INCREMENT=19 DEFAULT CHARSET=latin1;

-- 
-- Dumping data for table m_user
-- 

LOCK TABLES m_user WRITE;
INSERT INTO m_user VALUES
  (18, 'ADMINSPV', 'ADMINSPV', 'SUPER ADMIN', '21232f297a57a5a743894a0e4a801fc3', 1, 1);
UNLOCK TABLES;

-- 
-- Table structure for table  pelanggan
-- 

DROP TABLE IF EXISTS pelanggan;
CREATE TABLE `pelanggan` (
  `idpelanggan` int(11) NOT NULL AUTO_INCREMENT,
  `kodepelanggan` varchar(15) DEFAULT NULL,
  `namapelanggan` varchar(35) DEFAULT NULL,
  `alamat` varchar(100) DEFAULT NULL,
  `kota` varchar(25) DEFAULT NULL,
  `notelpon` varchar(20) DEFAULT NULL,
  `idjenispelanggan` int(11) DEFAULT NULL,
  `maxpiutang` decimal(20,2) DEFAULT '0.00',
  `top` int(11) DEFAULT '0',
  `poin` decimal(20,2) DEFAULT '0.00',
  `idsales` int(11) DEFAULT NULL,
  `maxjmlinvoice` int(11) DEFAULT NULL,
  `metodepiutang` enum('JUMLAH NOTA','MAX PIUTANG') DEFAULT 'JUMLAH NOTA',
  `tglinput` date DEFAULT NULL,
  PRIMARY KEY (`idpelanggan`)
) ENGINE=InnoDB AUTO_INCREMENT=480 DEFAULT CHARSET=latin1;

