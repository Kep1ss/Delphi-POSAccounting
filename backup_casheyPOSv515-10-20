CREATE TABLE `temp_stokopname` (
  `idstok` int(11) NOT NULL AUTO_INCREMENT,
  `nobukti` varchar(15) DEFAULT NULL,
  `tanggal` date DEFAULT NULL,
  `kodebarang` varchar(15) DEFAULT NULL,
  `satuan` varchar(25) DEFAULT NULL,
  `stokkomp` double DEFAULT NULL,
  `stoknyata` double DEFAULT NULL,
  `selisih` double DEFAULT NULL,
  `keterangan` varchar(250) DEFAULT NULL,
  `lastupdate` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP,
  `lastuser` int(11) DEFAULT NULL,
  `idgudang` int(11) DEFAULT NULL,
  `petugascheck` int(11) DEFAULT NULL,
  `sudahdiproses` tinyint(1) DEFAULT '0',
  PRIMARY KEY (`idstok`),
  KEY `tanggal` (`tanggal`),
  KEY `kodebarang` (`kodebarang`)
) ENGINE=InnoDB DEFAULT CHARSET=latin1;

-- 
-- Dumping data for table temp_stokopname
-- 


-- 
-- Table structure for table  tutupbulan
-- 

DROP TABLE IF EXISTS tutupbulan;
CREATE TABLE `tutupbulan` (
  `idtutupbulan` int(11) NOT NULL AUTO_INCREMENT,
  `tgl` date DEFAULT NULL,
  `noakun` varchar(100) DEFAULT NULL,
  `nominal` decimal(20,2) DEFAULT NULL,
  PRIMARY KEY (`idtutupbulan`)
) ENGINE=InnoDB DEFAULT CHARSET=latin1;

-- 
-- Dumping data for table tutupbulan
-- 


-- 
-- Table structure for table  tutupkasir
-- 

DROP TABLE IF EXISTS tutupkasir;
CREATE TABLE `tutupkasir` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `tgl` date DEFAULT NULL,
  `100rb` decimal(20,2) DEFAULT NULL,
  `50rb` decimal(20,2) DEFAULT NULL,
  `20rb` decimal(20,2) DEFAULT NULL,
  `10rb` decimal(20,2) DEFAULT NULL,
  `5rb` decimal(20,2) DEFAULT NULL,
  `2rb` decimal(20,2) DEFAULT NULL,
  `1rb` decimal(20,2) DEFAULT NULL,
  `500rupiah` decimal(20,2) DEFAULT NULL,
  `200rupiah` decimal(20,2) DEFAULT NULL,
  `100rupiah` decimal(20,2) DEFAULT NULL,
  PRIMARY KEY (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=latin1;

