-- 
-- Table structure for table  mutasibarang
-- 

DROP TABLE IF EXISTS mutasibarang;
CREATE TABLE `mutasibarang` (
  `idmutasi` int(11) NOT NULL AUTO_INCREMENT,
  `nobukti` varchar(15) DEFAULT NULL,
  `tgl` date DEFAULT NULL,
  `iduser` int(11) DEFAULT NULL,
  `darigudang` varchar(15) DEFAULT NULL,
  `kegudang` varchar(15) DEFAULT NULL,
  `totalbarang` decimal(20,2) DEFAULT NULL,
  `keterangan` varchar(200) DEFAULT NULL,
  PRIMARY KEY (`idmutasi`)
) ENGINE=InnoDB DEFAULT CHARSET=latin1;

