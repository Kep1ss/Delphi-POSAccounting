-- 
-- StoredProc  sp_QtyPembelianBarangBulan
-- 

DELIMITER $$
DROP PROCEDURE IF EXISTS sp_QtyPembelianBarangBulan $$
CREATE DEFINER=`root`@`localhost` PROCEDURE `sp_QtyPembelianBarangBulan`(IN thn VARCHAR(6))
BEGIN
  DECLARE finish INT DEFAULT 0 ;
  DECLARE cdate VARCHAR(20) ;
  DECLARE strCheck VARCHAR (5);
  DECLARE str VARCHAR (10000) DEFAULT '' ;
  DECLARE curs CURSOR FOR 
  SELECT 
   DISTINCT DATE_FORMAT(tgl,'%b') 
  FROM
    pembelian 
  WHERE YEAR(tgl)>= thn ;
    
  DECLARE CONTINUE HANDLER FOR NOT FOUND SET finish = 1 ;
  
  SET str = CONCAT('select "',thn,'" as "TAHUN",s.namasupplier as 
  "SUPPLIER",b.namabarang as "NAMA BARANG",');
  OPEN curs ;
  my_loop :
  LOOP
    FETCH curs INTO cdate ;
    IF finish = 1 
    THEN LEAVE my_loop ;
    END IF ;
    SET str = CONCAT(
      str,
      ' SUM(case when DATE_FORMAT(p.tgl,"%b") ="',
      cdate,
      '" then IFNULL(pd.jumlah,0) else 0 end) as "',
      cdate,
      '",'
    ) ;
  END LOOP ;
  CLOSE curs ;
  SET str = SUBSTR(str, 1, CHAR_LENGTH(str) - 1) ;
  
  SET strCheck = RIGHT(str,1);
  
  IF strCheck <>',' THEN
    SET str =CONCAT(str,',');
  END IF;
  SET @str = CONCAT(
    str,
    ' SUM(IFNULL(pd.jumlah,0)) as TOTAL from m_supplier s LEFT JOIN pembelian p 
    ON s.idsupplier=p.idsupplier  
    LEFT JOIN detailbeli pd ON pd.nobukti=p.nobukti
    LEFT JOIN barang b ON b.kodebarang=pd.kodebarang 
    WHERE YEAR(p.tgl)="',thn,'" OR p.idsupplier is null
    GROUP BY pd.kodebarang,p.idsupplier'
  ) ;
  PREPARE stmt FROM @str ;
  EXECUTE stmt ;
  DEALLOCATE PREPARE stmt ;
END $$
DELIMITER ;


-- 
-- StoredProc  sp_QtyPembelianBarangHari
-- 

DELIMITER $$
DROP PROCEDURE IF EXISTS sp_QtyPembelianBarangHari $$
CREATE DEFINER=`root`@`localhost` PROCEDURE `sp_QtyPembelianBarangHari`(IN TGL1 DATE,IN TGL2 DATE)
BEGIN
  DECLARE finish INT DEFAULT 0 ;
  DECLARE cdate DATE ;
  DECLARE strCheck VARCHAR (5);
  DECLARE str VARCHAR (10000) DEFAULT '' ;
  DECLARE curs CURSOR FOR 
  SELECT 
   DISTINCT tgl
  FROM
    pembelian 
  WHERE tgl>= TGL1
    AND tgl<= TGL2 ;
    
  DECLARE CONTINUE HANDLER FOR NOT FOUND SET finish = 1 ;
  
  SET str = CONCAT('select "',DATE_FORMAT(TGL2,'%b'),'" as "BULAN",s.namasupplier as 
  "SUPPLIER",b.namabarang as "NAMA BARANG",(CASE WHEN k.namakategori="BIBIT" then "BIBIT" else "NON BIBIT" END) as "KATEGORI",');
  OPEN curs ;
  my_loop :
  LOOP
    FETCH curs INTO cdate ;
    IF finish = 1 
    THEN LEAVE my_loop ;
    END IF ;
    SET str = CONCAT(
      str,
      ' SUM(case when p.tgl ="',
      DATE(cdate),
      '" then IFNULL(pd.jumlah,0) else 0 end) as "',
      DATE_FORMAT(cdate,"%d %b %y"),
      '",'
    ) ;
  END LOOP ;
  CLOSE curs ;
  SET str = SUBSTR(str, 1, CHAR_LENGTH(str) - 1) ;
  
  SET strCheck = RIGHT(str,1);
  
  IF strCheck <>',' THEN
    SET str =CONCAT(str,',');
  END IF;
  SET @str = CONCAT(
    str,
    ' SUM(IFNULL(pd.jumlah,0)) as TOTAL from m_supplier s LEFT JOIN pembelian p 
    ON s.idsupplier=p.idsupplier
    LEFT JOIN detailbeli  pd ON pd.nobukti=p.nobukti
    LEFT JOIN barang b ON b.kodebarang=pd.kodebarang LEFT JOIN kategori k ON k.idkategori=b.idkategori 
    WHERE (p.idsupplier IS NULL OR (p.tgl>="',tgl1,'" AND p.tgl<="',tgl2,'"))
    GROUP BY pd.kodebarang,p.idsupplier'
  ) ;
  PREPARE stmt FROM @str ;
  EXECUTE stmt ;
  DEALLOCATE PREPARE stmt ;
END $$
DELIMITER ;


-- 
-- StoredProc  sp_rekapBarang
-- 

DELIMITER $$
DROP PROCEDURE IF EXISTS sp_rekapBarang $$
CREATE DEFINER=`root`@`localhost` PROCEDURE `sp_rekapBarang`(IN thn VARCHAR(6))
BEGIN
  DECLARE finish INT DEFAULT 0 ;
  DECLARE cdate VARCHAR(20) ;
  DECLARE strCheck VARCHAR (5);
  DECLARE str VARCHAR (10000) DEFAULT '' ;
  DECLARE curs CURSOR FOR 
  SELECT 
   DISTINCT DATE_FORMAT(tgl,'%b') 
  FROM
    kartustok 
  WHERE YEAR(tgl)>= thn ;
    
  DECLARE CONTINUE HANDLER FOR NOT FOUND SET finish = 1 ;
  
  SET str = CONCAT('select "',thn,'" as "TAHUN",b.kodebarang as "KODE BARANG",b.namabarang as "NAMA BARANG",');
  OPEN curs ;
  my_loop :
  LOOP
    FETCH curs INTO cdate ;
    IF finish = 1 
    THEN LEAVE my_loop ;
    END IF ;
    SET str = CONCAT(
      str,
      ' SUM(case when DATE_FORMAT(k.tgl,"%b") ="',
      cdate,
      '" then IFNULL(k.masuk,0) else 0 end) as "',
      cdate,
      '_(In)",'
      ' SUM(case when DATE_FORMAT(k.tgl,"%b") ="',
      cdate,
      '" then IFNULL(k.keluar,0) else 0 end) as "',
      cdate,
      '_(Out)",'
    ) ;
  END LOOP ;
  CLOSE curs ;
  SET str = SUBSTR(str, 1, CHAR_LENGTH(str) - 1) ;
  
  SET strCheck = RIGHT(str,1);
  
  IF strCheck <>',' THEN
    SET str =CONCAT(str,',');
  END IF;
  SET @str = CONCAT(
    str,
    ' SUM(IFNULL(k.masuk,0))-SUM(IFNULL(k.keluar,0)) as "STOK" from kartustok k LEFT JOIN barang b 
    ON b.kodebarang=k.kodebarang
    WHERE YEAR(k.tgl)="',thn,'"
    GROUP BY k.kodebarang'
  ) ;
  PREPARE stmt FROM @str ;
  EXECUTE stmt ;
  DEALLOCATE PREPARE stmt ;
END $$
DELIMITER ;


-- 
-- StoredProc  sp_rekapitulasikasir
-- 

DELIMITER $$
DROP PROCEDURE IF EXISTS sp_rekapitulasikasir $$
CREATE DEFINER=`root`@`localhost` PROCEDURE `sp_rekapitulasikasir`(in tanggal1 date,in tanggal2 date)
BEGIN
	select u.username,p.tgljual,
	SUM(CASE WHEN p.pembayaran = 0 then p.grandtotal else 0 end) as totalCash,
	SUM(Case when p.pembayaran = 1 then p.grandtotal else 0 end) as totalkredit
	from penjualan p left join m_user u on p.iduser=u.iduser
	where p.tgljual>=tanggal1 AND p.tgljual<=tanggal2 group by p.iduser;
    END $$
DELIMITER ;


-- 
-- StoredProc  sp_returjual
-- 

DELIMITER $$
DROP PROCEDURE IF EXISTS sp_returjual $$
CREATE DEFINER=`root`@`localhost` PROCEDURE `sp_returjual`(IN tgl1 DATE, IN tgl2 DATE)
BEGIN
  DECLARE finish INT DEFAULT 0 ;
  DECLARE cdate DATE ;
  DECLARE aAkun VARCHAR (10) ;
  DECLARE aNamaAkun VARCHAR (100) ;
  DECLARE aNominal DECIMAL (20, 2) ;
  DECLARE str TEXT ;
  DECLARE cur CURSOR FOR 
  SELECT 
    kodeakunperkiraan,
    namaakunperkiraan 
  FROM
    m_akunperkiraan 
  WHERE kodeakunperkiraan LIKE "503%" ;
  
  DECLARE CONTINUE HANDLER FOR NOT FOUND SET finish = 1;
  DROP TABLE IF EXISTS tmp_retur ;
  CREATE TEMPORARY TABLE tmp_retur (
    id INT NOT NULL AUTO_INCREMENT,
    kodeakunperkiraan VARCHAR (10),
    namaakunperkiraan VARCHAR (200),
    saldo DECIMAL (20, 2),
    PRIMARY KEY (id)
  ) ;
  
  OPEN cur ;
  curLoop :
  LOOP
	FETCH cur INTO aAkun,aNamaAkun;
	IF finish = 1 THEN LEAVE curLoop;
	END IF;
	SELECT 
      IFNULL(SUM(s_awal + debet - kredit), 0) INTO aNominal 
    FROM
      saldoakun 
    WHERE noakun = aAkun 
      AND tgl >= tgl1 
      AND tgl <= tgl2 ;
      
      INSERT INTO tmp_retur (
      kodeakunperkiraan,
      namaakunperkiraan,
      saldo
    ) 
    VALUES
      (aAkun, aNamaAkun, aNominal) ;
  END LOOP ;
  CLOSE cur ;
  
  SELECT 
    * 
  FROM
    tmp_retur ;
    END $$
DELIMITER ;


-- 
-- StoredProc  sp_stokbarang
-- 

DELIMITER $$
DROP PROCEDURE IF EXISTS sp_stokbarang $$
CREATE DEFINER=`root`@`localhost` PROCEDURE `sp_stokbarang`(in tgl1 date, in tgl2 date,isCari VarChar(200),in isCari2 Varchar(200))
BEGIN
    DECLARE finish INTEGER DEFAULT 0 ;
  DECLARE str VARCHAR (1000) ;
  DECLARE strCheck VARCHAR (5);
  DECLARE GudangName VARCHAR (50) ;
  DECLARE aGudang CURSOR FOR 
  SELECT DISTINCT 
    namalokasi 
  FROM
    lokasi ;
  DECLARE CONTINUE HANDLER FOR NOT FOUND SET finish = 1 ;
  SET Str = 'SELECT b.kodebarang as "Kode Barang",b.kodebarcode as "Kode Barcode",b.namabarang as "Nama Barang",
  k.namakategori as "Kategori",hr.hargabeli + (hr.hargabeli * b.ppn /100) as "Harga Beli(+PPN)",b.hargaecer as "Harga Jual",' ;
  OPEN aGudang ;
  Loop_Gudang :
  LOOP
    FETCH aGudang INTO GudangName ;
    IF Finish = 1 
    THEN LEAVE Loop_Gudang ;
    END IF ;
    SET str = CONCAT(
      str,
      'SUM(CASE WHEN l.namalokasi="',
      GudangName,
      '" THEN ((ks.saldoawal + ks.masuk) - ks.keluar) ElSE 0 END) AS `',
      GudangName,
      '`,'
    ) ;
  END LOOP ;
  CLOSE aGudang ;
  SET str = SUBSTR(str, 1, CHAR_LENGTH(str) - 1) ;
  
  SET strCheck = RIGHT(str,1);
  
  IF strCheck <>',' THEN
    SET str =CONCAT(str,',');
  END IF;
  
  SET @str = CONCAT(
    str,
    '(SUM(ks.saldoawal+ks.masuk-ks.keluar)) as "TOTAL STOK ",
    (SUM(ks.saldoawal + ks.masuk - ks.keluar) * hr.hargabeli + (hr.hargabeli * b.ppn /100)) as "TOTAL HARGA"  FROM barang b LEFT JOIN kategori k ON k.idkategori=b.idkategori
				   LEFT JOIN kartustok ks 
				   ON ks.kodebarang=b.kodebarang LEFT JOIN lokasi l ON l.idlokasi=ks.idgudang 
				   LEFT JOIN hargabelirata hr ON hr.kodebarang=b.kodebarang WHERE ks.tgl>="',
    tgl1,
    '" AND ks.tgl<="',
    tgl2,
    '" ',isCari,' GROUP by b.kodebarang',iscari2,' ') ;
  
 
  
  PREPARE stmt FROM @str ;
  EXECUTE stmt ;
  DEALLOCATE PREPARE stmt ;
END $$
DELIMITER ;


-- 
-- StoredProc  sp_stokbarangx
-- 

DELIMITER $$
DROP PROCEDURE IF EXISTS sp_stokbarangx $$
CREATE DEFINER=`root`@`localhost` PROCEDURE `sp_stokbarangx`(IN tgl1 DATE, IN tgl2 DATE,isCari VARCHAR(200),IN isCari2 VARCHAR(200))
BEGIN
  DECLARE finish INTEGER DEFAULT 0 ;
  DECLARE str VARCHAR (1000) ;
  DECLARE strCheck VARCHAR (5);
  DECLARE GudangName VARCHAR (50) ;
  DECLARE aGudang CURSOR FOR 
  SELECT DISTINCT 
    namalokasi 
  FROM
    lokasi ;
  DECLARE CONTINUE HANDLER FOR NOT FOUND SET finish = 1 ;
  SET Str = 'SELECT b.kodebarang as "Kode Barang",b.kodebarcode as "Kode Barcode",b.namabarang as "Nama Barang",
  k.namakategori as "Kategori",b.hargaecer as "Harga Jual",' ;
  OPEN aGudang ;
  Loop_Gudang :
  LOOP
    FETCH aGudang INTO GudangName ;
    IF Finish = 1 
    THEN LEAVE Loop_Gudang ;
    END IF ;
    SET str = CONCAT(
      str,
      'SUM(CASE WHEN l.namalokasi="',
      GudangName,
      '" THEN ((ks.saldoawal + ks.masuk) - ks.keluar) ElSE 0 END) AS `',
      GudangName,
      '`,'
    ) ;
  END LOOP ;
  CLOSE aGudang ;
  SET str = SUBSTR(str, 1, CHAR_LENGTH(str) - 1) ;
  
  SET strCheck = RIGHT(str,1);
  
  IF strCheck <>',' THEN
    SET str =CONCAT(str,',');
  END IF;
  
  SET @str = CONCAT(
    str,
    '(SUM(ks.saldoawal+ks.masuk-ks.keluar)) as "TOTAL STOK "  FROM barang b LEFT JOIN kategori k ON k.idkategori=b.idkategori
				   LEFT JOIN kartustok ks 
				   ON ks.kodebarang=b.kodebarang LEFT JOIN lokasi l ON l.idlokasi=ks.idgudang 
				   WHERE ks.tgl>="',
    tgl1,
    '" AND ks.tgl<="',
    tgl2,
    '" ',isCari,' GROUP by b.kodebarang',iscari2,' ') ;
  
 
  
  PREPARE stmt FROM @str ;
  EXECUTE stmt ;
  DEALLOCATE PREPARE stmt ;
END $$
DELIMITER ;


-- 
-- StoredProc  sp_stokonline
-- 

DELIMITER $$
DROP PROCEDURE IF EXISTS sp_stokonline $$
CREATE DEFINER=`root`@`localhost` PROCEDURE `sp_stokonline`(IN tgl1 DATE, IN tgl2 DATE,isCari VARCHAR(200))
BEGIN
  DECLARE finish INTEGER DEFAULT 0 ;
  DECLARE str VARCHAR (1000) ;
  DECLARE strCheck VARCHAR (5);
  DECLARE GudangName VARCHAR (50) ;
  DECLARE aGudang CURSOR FOR 
  SELECT DISTINCT 
    namalokasi 
  FROM
    lokasi ;
  DECLARE CONTINUE HANDLER FOR NOT FOUND SET finish = 1 ;
  SET Str = 'SELECT b.kodebarang,b.namabarang,b.hargaecer,b.hargagrosir,b.hargapartai, ' ;
  OPEN aGudang ;
  Loop_Gudang :
  LOOP
    FETCH aGudang INTO GudangName ;
    IF Finish = 1 
    THEN LEAVE Loop_Gudang ;
    END IF ;
    SET str = CONCAT(
      str,
      'SUM(CASE WHEN l.namalokasi="',
      GudangName,
      '" THEN (ks.saldoawal + ks.masuk - ks.keluar) ElSE 0 END) AS `',
      GudangName,
      '`,'
    ) ;
  END LOOP ;
  CLOSE aGudang ;
  SET str = SUBSTR(str, 1, CHAR_LENGTH(str) - 1) ;
  
  
  SET @str = CONCAT(
    str,
    ' FROM barang b LEFT JOIN kategori k ON k.idkategori=b.idkategori
				   LEFT JOIN kartustok ks 
				   ON ks.kodebarang=b.kodebarang LEFT JOIN lokasi l ON l.idlokasi=ks.idgudang 
				   WHERE ks.tgl>="',
    tgl1,
    '" AND ks.tgl<="',
    tgl2,
    '" ',isCari,' GROUP by ks.kodebarang') ;
  
 
  
  PREPARE stmt FROM @str ;
  EXECUTE stmt ;
  DEALLOCATE PREPARE stmt ;
  END $$
DELIMITER ;


-- 
-- StoredProc  sp_TahunPembelian
-- 

DELIMITER $$
DROP PROCEDURE IF EXISTS sp_TahunPembelian $$
CREATE DEFINER=`root`@`localhost` PROCEDURE `sp_TahunPembelian`(in thn Varchar(6))
BEGIN
  DECLARE finish INT DEFAULT 0 ;
  DECLARE cdate Varchar(20) ;
  declare strCheck varchar (5);
  DECLARE str VARCHAR (10000) DEFAULT 'select YEAR(p.tgl) as "TAHUN",s.namasupplier as "SUPPLIER",' ;
  DECLARE curs CURSOR FOR 
  SELECT 
   DISTINCT DATE_FORMAT(tgl,'%b')
  FROM
    pembelian 
  WHERE YEAR(tgl)>= thn;
    
  DECLARE CONTINUE HANDLER FOR NOT FOUND SET finish = 1 ;
  OPEN curs ;
  my_loop :
  LOOP
    FETCH curs INTO cdate ;
    IF finish = 1 
    THEN LEAVE my_loop ;
    END IF ;
    SET str = CONCAT(
      str,
      ' SUM(case when DATE_FORMAT(p.tgl,"%b") ="',
      cdate,
      '" then p.total else 0 end) as "',
      cdate,
      '",'
    ) ;
  END LOOP ;
  CLOSE curs ;
  SET str = SUBSTR(str, 1, CHAR_LENGTH(str) - 1) ;
  
  SET strCheck = right(str,1);
  
  if strCheck <>',' then
    SET str =CONCAT(str,',');
  end if;
  SET @str = CONCAT(
    str,
    ' SUM(p.total) as TOTAL from pembelian p 
    LEFT JOIN m_supplier s ON s.idsupplier=p.idsupplier
    where YEAR(p.tgl)>="',
    thn,
    '" GROUP BY s.idsupplier ORDER BY s.namasupplier ASC'
  ) ;
  PREPARE stmt FROM @str ;
  EXECUTE stmt ;
  DEALLOCATE PREPARE stmt ;
END $$
DELIMITER ;


-- 
-- Table structure for table  barang
-- 

DROP TABLE IF EXISTS barang;
CREATE TABLE `barang` (
  `idbarang` int(11) NOT NULL AUTO_INCREMENT,
  `kodebarang` varchar(50) DEFAULT NULL,
  `kodebarcode` varchar(50) DEFAULT NULL,
  `namabarang` varchar(200) DEFAULT NULL,
  `namabarcode` varchar(200) DEFAULT NULL,
  `idkategori` int(11) DEFAULT NULL,
  `satuanbeli` varchar(15) DEFAULT NULL,
  `satuanjual` varchar(15) DEFAULT NULL,
  `isi` decimal(20,2) DEFAULT '0.00',
  `stokmin` decimal(20,2) DEFAULT '0.00',
  `stokmax` decimal(20,2) DEFAULT '0.00',
  `minimalorder` decimal(20,2) DEFAULT '0.00',
  `hargasebelum` decimal(20,2) DEFAULT '0.00',
  `hargabeli` decimal(20,2) DEFAULT '0.00',
  `margin` decimal(20,2) unsigned DEFAULT '0.00',
  `hargaecer` decimal(20,2) DEFAULT '0.00',
  `margingrosir` decimal(20,2) DEFAULT '0.00',
  `hargagrosir` decimal(20,2) DEFAULT '0.00',
  `marginpartai` decimal(20,2) DEFAULT '0.00',
  `hargapartai` decimal(20,2) DEFAULT '0.00',
  `hargapartai4` decimal(20,2) DEFAULT '0.00',
  `hargapartai5` decimal(20,2) DEFAULT '0.00',
  `diskonpersen` decimal(3,0) DEFAULT '0',
  `diskonrupiah` decimal(20,2) DEFAULT '0.00',
  `ppn` decimal(3,0) DEFAULT '0',
  `createby` int(11) DEFAULT NULL,
  `lastupdate` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP,
  `isDelete` tinyint(1) DEFAULT '0',
  `idsupplier` int(11) DEFAULT NULL,
  `warningiflimit` tinyint(1) DEFAULT '1',
  PRIMARY KEY (`idbarang`),
  UNIQUE KEY `kodebarang` (`kodebarang`),
  UNIQUE KEY `kodebarcode` (`kodebarcode`),
  KEY `idkategori` (`idkategori`),
  KEY `namabarang` (`namabarang`)
) ENGINE=InnoDB AUTO_INCREMENT=602 DEFAULT CHARSET=latin1;

