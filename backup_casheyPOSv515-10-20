-- Table structure for table  detailbeli
-- 

DROP TABLE IF EXISTS detailbeli;
CREATE TABLE `detailbeli` (
  `iddetilbeli` int(11) NOT NULL AUTO_INCREMENT,
  `nobukti` varchar(50) DEFAULT NULL,
  `kodebarang` varchar(15) DEFAULT NULL,
  `satuan` varchar(15) DEFAULT NULL,
  `harga` decimal(20,2) DEFAULT NULL,
  `hargaecer` decimal(20,2) DEFAULT NULL,
  `hargagrosir` decimal(20,2) DEFAULT NULL,
  `hargapartai` decimal(20,2) DEFAULT NULL,
  `jumlah` decimal(20,2) DEFAULT '0.00',
  `masuk` decimal(20,2) DEFAULT '0.00',
  `retur` decimal(20,2) DEFAULT '0.00',
  `diskonpersen` decimal(20,2) DEFAULT NULL,
  `diskonrupiah` decimal(20,2) DEFAULT NULL,
  `diskon2` decimal(20,2) DEFAULT NULL,
  `diskon3` decimal(20,2) DEFAULT NULL,
  `ppn` decimal(20,2) DEFAULT NULL,
  `subtotal` decimal(20,2) DEFAULT NULL,
  `isi` decimal(20,2) DEFAULT NULL,
  `idgudang` int(11) DEFAULT '1',
  PRIMARY KEY (`iddetilbeli`),
  KEY `kodebarang` (`kodebarang`),
  KEY `idgudang` (`idgudang`),
  KEY `detailbeli_ibfk_3` (`nobukti`),
  CONSTRAINT `detailbeli_ibfk_1` FOREIGN KEY (`kodebarang`) REFERENCES `barang` (`kodebarang`) ON DELETE CASCADE ON UPDATE CASCADE,
  CONSTRAINT `detailbeli_ibfk_2` FOREIGN KEY (`idgudang`) REFERENCES `lokasi` (`idlokasi`) ON DELETE CASCADE ON UPDATE CASCADE,
  CONSTRAINT `detailbeli_ibfk_3` FOREIGN KEY (`nobukti`) REFERENCES `pembelian` (`nobukti`) ON DELETE CASCADE ON UPDATE CASCADE
) ENGINE=InnoDB DEFAULT CHARSET=latin1;

