-- 
-- Table structure for table  multiqty
-- 

DROP TABLE IF EXISTS multiqty;
CREATE TABLE `multiqty` (
  `idmultiqty` int(11) NOT NULL AUTO_INCREMENT,
  `kodebarang` varchar(15) DEFAULT NULL,
  `qtyawal` int(11) DEFAULT NULL,
  `qtyakhir` int(11) DEFAULT NULL,
  `harga` decimal(20,2) DEFAULT NULL,
  PRIMARY KEY (`idmultiqty`),
  KEY `kodebarang` (`kodebarang`),
  CONSTRAINT `multiqty_ibfk_1` FOREIGN KEY (`kodebarang`) REFERENCES `barang` (`kodebarang`) ON DELETE CASCADE ON UPDATE CASCADE
) ENGINE=InnoDB DEFAULT CHARSET=latin1;

