-- 
-- Table structure for table  pendingso
-- 

DROP TABLE IF EXISTS pendingso;
CREATE TABLE `pendingso` (
  `idjual` int(11) NOT NULL AUTO_INCREMENT,
  `nosalesorder` varchar(25) DEFAULT NULL,
  `tgljual` date DEFAULT NULL,
  `idpelanggan` int(11) DEFAULT NULL,
  `pembayaran` int(11) DEFAULT '0',
  `iduser` int(11) DEFAULT NULL,
  `komputerid` int(11) DEFAULT NULL,
  `idsales` int(11) DEFAULT NULL,
  PRIMARY KEY (`idjual`),
  KEY `tgljual` (`tgljual`),
  KEY `idpelanggan` (`idpelanggan`),
  KEY `noso` (`nosalesorder`)
) ENGINE=InnoDB DEFAULT CHARSET=latin1;

-- 
-- Dumping data for table pendingso
-- 


-- 
-- Table structure for table  pendingsodetail
-- 

DROP TABLE IF EXISTS pendingsodetail;
CREATE TABLE `pendingsodetail` (
  `iddetil` int(11) NOT NULL AUTO_INCREMENT,
  `nosalesorder` varchar(25) DEFAULT NULL,
  `kodebarang` varchar(15) DEFAULT NULL,
  `satuan` varchar(25) DEFAULT NULL,
  `jumlah` double(11,2) DEFAULT NULL,
  `isi` double(11,2) DEFAULT NULL,
  `idgudang` int(11) DEFAULT NULL,
  PRIMARY KEY (`iddetil`),
  KEY `kodebarang` (`kodebarang`),
  KEY `idgudang` (`idgudang`),
  KEY `noso` (`nosalesorder`)
) ENGINE=InnoDB DEFAULT CHARSET=latin1;

-- 
-- Dumping data for table pendingsodetail
-- 


-- 
-- Table structure for table  penerimaanbarang
-- 

DROP TABLE IF EXISTS penerimaanbarang;
CREATE TABLE `penerimaanbarang` (
  `idpenerimaan` int(11) NOT NULL AUTO_INCREMENT,
  `nobukti` varchar(50) DEFAULT NULL,
  `tgl` date DEFAULT NULL,
  `nobuktibeli` varchar(50) DEFAULT NULL,
  `nobuktiorder` varchar(15) DEFAULT NULL,
  `keterangan` varchar(200) DEFAULT NULL,
  `nopo` varchar(15) DEFAULT NULL,
  PRIMARY KEY (`idpenerimaan`),
  UNIQUE KEY `nobukti` (`nobukti`)
) ENGINE=InnoDB DEFAULT CHARSET=latin1;

-- 
-- Dumping data for table penerimaanbarang
-- 


-- 
-- Table structure for table  penerimaandetail
-- 

DROP TABLE IF EXISTS penerimaandetail;
CREATE TABLE `penerimaandetail` (
  `iddetailterima` int(11) NOT NULL AUTO_INCREMENT,
  `nobukti` varchar(50) DEFAULT NULL,
  `kodebarang` varchar(35) DEFAULT NULL,
  `satuan` varchar(25) DEFAULT NULL,
  `harga` decimal(20,2) DEFAULT NULL,
  `jmlorder` decimal(20,2) DEFAULT NULL,
  `masuk` decimal(20,2) DEFAULT NULL,
  `kekurangan` decimal(20,2) DEFAULT NULL,
  `idgudang` int(11) DEFAULT '1',
  `isi` decimal(20,2) DEFAULT NULL,
  PRIMARY KEY (`iddetailterima`),
  KEY `nobukti` (`nobukti`),
  CONSTRAINT `penerimaandetail_ibfk_1` FOREIGN KEY (`nobukti`) REFERENCES `penerimaanbarang` (`nobukti`) ON DELETE CASCADE ON UPDATE CASCADE
) ENGINE=InnoDB DEFAULT CHARSET=latin1;

-- 
-- Dumping data for table penerimaandetail
-- 


-- 
-- Table structure for table  pengeluaranbiaya
-- 

DROP TABLE IF EXISTS pengeluaranbiaya;
CREATE TABLE `pengeluaranbiaya` (
  `idpengeluaran` int(11) NOT NULL AUTO_INCREMENT,
  `nobukti` varchar(15) DEFAULT NULL,
  `tanggal` date DEFAULT NULL,
  `noakun` varchar(10) DEFAULT NULL,
  `nominal` decimal(20,2) DEFAULT NULL,
  `keterangan` varchar(200) DEFAULT NULL,
  `iduser` int(11) DEFAULT NULL,
  `stts` enum('O','P','C') DEFAULT NULL,
  PRIMARY KEY (`idpengeluaran`),
  UNIQUE KEY `nobukti` (`nobukti`),
  KEY `tanggal` (`tanggal`)
) ENGINE=InnoDB DEFAULT CHARSET=latin1;

-- 
-- Dumping data for table pengeluaranbiaya
-- 


-- 
-- Table structure for table  penjualan
-- 

DROP TABLE IF EXISTS penjualan;
CREATE TABLE `penjualan` (
  `idjual` int(11) NOT NULL AUTO_INCREMENT,
  `fakturjual` varchar(25) DEFAULT NULL,
  `tgljual` date DEFAULT NULL,
  `idpelanggan` int(11) DEFAULT NULL,
  `pembayaran` int(11) DEFAULT '0',
  `nomerso` varchar(30) DEFAULT '0',
  `jenisbayar` varchar(15) DEFAULT NULL,
  `diskon` int(11) DEFAULT NULL,
  `total` double DEFAULT NULL,
  `pembulatan` double(11,2) DEFAULT NULL,
  `grandtotal` double(11,2) DEFAULT NULL,
  `lastupdate` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP,
  `lastuser` int(11) DEFAULT NULL,
  `nontunai` decimal(20,2) DEFAULT NULL,
  `kodevoucher` varchar(30) DEFAULT NULL,
  `nominalvoucher` decimal(20,2) DEFAULT NULL,
  `bayar` double(11,2) DEFAULT NULL,
  `kembali` double(11,2) DEFAULT NULL,
  `totalprofit` double(11,2) DEFAULT NULL,
  `returjual` int(11) DEFAULT '0',
  `iduser` int(11) DEFAULT NULL,
  `komputerid` int(11) DEFAULT NULL,
  `nosalesorder` varchar(25) DEFAULT NULL,
  `idsales` int(11) DEFAULT NULL,
  `ppn` decimal(20,2) DEFAULT NULL,
  `tgltempo` date DEFAULT NULL,
  `tglkirim` date DEFAULT NULL,
  `kirim` tinyint(1) DEFAULT '0',
  `edc` varchar(100) DEFAULT NULL,
  `kartu` varchar(100) DEFAULT NULL,
  `sucharge` decimal(20,2) DEFAULT NULL,
  `totalnontunai` decimal(20,2) DEFAULT NULL,
  `nokartu` varchar(200) DEFAULT NULL,
  PRIMARY KEY (`idjual`),
  UNIQUE KEY `fakturjual` (`fakturjual`),
  KEY `tgljual` (`tgljual`),
  KEY `idpelanggan` (`idpelanggan`),
  KEY `lastuser` (`lastuser`)
) ENGINE=InnoDB AUTO_INCREMENT=42 DEFAULT CHARSET=latin1;

