-- 
-- Table structure for table  harmodal
-- 

DROP TABLE IF EXISTS harmodal;
CREATE TABLE `harmodal` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `tglinput` date DEFAULT NULL,
  `nofaktur` varchar(15) DEFAULT NULL,
  `tibatgl` date DEFAULT NULL,
  `supplier` varchar(50) DEFAULT NULL,
  `namakapal` varchar(100) DEFAULT NULL,
  PRIMARY KEY (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=latin1;

-- 
-- Dumping data for table harmodal
-- 


-- 
-- Table structure for table  hasilperhitungan
-- 

DROP TABLE IF EXISTS hasilperhitungan;
CREATE TABLE `hasilperhitungan` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `nofaktur` varchar(15) DEFAULT NULL,
  `nokartu` varchar(15) DEFAULT NULL,
  `namabarang` varchar(100) DEFAULT NULL,
  `qty` decimal(20,2) DEFAULT NULL,
  `beli` decimal(20,2) DEFAULT NULL,
  `angkutan` decimal(20,2) DEFAULT NULL,
  `ppn` decimal(20,2) DEFAULT NULL,
  `ppnrp` decimal(20,2) DEFAULT NULL,
  `labapersen` decimal(20,2) DEFAULT NULL,
  `labarp` decimal(20,2) DEFAULT NULL,
  `modal` decimal(20,2) DEFAULT NULL,
  `hargaecer` decimal(20,2) DEFAULT NULL,
  `hargagrosir` decimal(20,2) DEFAULT NULL,
  `hargapartai` decimal(20,2) DEFAULT NULL,
  `hargapartai4` decimal(20,2) DEFAULT NULL,
  `hargapartai5` decimal(20,2) DEFAULT NULL,
  `supplier` varchar(50) DEFAULT NULL,
  `kategori` varchar(25) DEFAULT NULL,
  `akhirupdate` date DEFAULT NULL,
  PRIMARY KEY (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=latin1;

