-- 
-- Table structure for table  v_penjualan
-- 

DROP VIEW IF EXISTS v_penjualan;
CREATE ALGORITHM=UNDEFINED DEFINER=`root`@`localhost` SQL SECURITY DEFINER VIEW `v_penjualan` AS (select `p`.`fakturjual` AS `fakturjual`,`p`.`tgljual` AS `tgljual`,`s`.`namasales` AS `namasales`,`pl`.`namapelanggan` AS `namapelanggan`,`pl`.`alamat` AS `alamat`,`pl`.`kota` AS `kota`,(case when (`p`.`pembayaran` = 0) then 'CASH' else 'KREDIT' end) AS `Pembayaran`,concat((to_days(curdate()) - to_days(`p`.`tgltempo`)),' ','Hari') AS `tempo`,`p`.`total` AS `total`,`p`.`diskon` AS `diskon`,`p`.`ppn` AS `ppn`,`p`.`grandtotal` AS `grandtotal`,`pd`.`kodebarang` AS `kodebarcode`,`b`.`namabarang` AS `namabarang`,`pd`.`satuan` AS `satuan`,`pd`.`harga` AS `harga`,`pd`.`jumlah` AS `jumlah`,`pd`.`diskonpersen` AS `diskonpersen`,`pd`.`diskon2` AS `diskon2`,`pd`.`diskon3` AS `diskon3`,`pd`.`subtotal` AS `subtotal`,`pd`.`isi` AS `isi` from ((((`penjualan` `p` left join `penjualandetil` `pd` on((`p`.`fakturjual` = `pd`.`fakturjual`))) left join `sales` `s` on((`s`.`idsales` = `p`.`idsales`))) left join `pelanggan` `pl` on((`pl`.`idpelanggan` = `p`.`idpelanggan`))) left join `barang` `b` on((`b`.`kodebarang` = `pd`.`kodebarang`))));


-- 
-- Table structure for table  v_returjual
-- 

DROP VIEW IF EXISTS v_returjual;
CREATE ALGORITHM=UNDEFINED DEFINER=`root`@`localhost` SQL SECURITY DEFINER VIEW `v_returjual` AS (select `rj`.`nobukti` AS `nobukti`,`rj`.`tgl` AS `tgl`,`rj`.`total` AS `totalretur`,`pl`.`namapelanggan` AS `namapelanggan`,`rd`.`kodebarang` AS `kdbarcode`,`b`.`namabarang` AS `nmbarang`,`rd`.`satuan` AS `satuan`,`rd`.`harga` AS `harga`,`rd`.`diskonpersen` AS `diskonpersen`,`rd`.`diskon2` AS `diskon2`,`rd`.`diskon3` AS `diskon3`,`rd`.`jumlah` AS `jumlah`,`rd`.`subtotal` AS `subtotal`,`rd`.`isi` AS `isi` from (((`returpenjualan` `rj` left join `detailreturjual` `rd` on((`rd`.`nobukti` = `rj`.`nobukti`))) left join `barang` `b` on((`b`.`kodebarang` = `rd`.`kodebarang`))) left join `pelanggan` `pl` on((`pl`.`idpelanggan` = `rj`.`idpelanggan`))));


DELIMITER $$
-- 
-- Trigger  tg_batalPoin
-- 

CREATE TRIGGER tg_batalPoin AFTER INSERT ON returpenjualan FOR EACH ROW
BEGIN
	DECLARE minPoinPlg INT;
	
	SELECT minpoin INTO minPoinPlg FROM profile WHERE idprofil=1;
	
	UPDATE pelanggan SET poin=poin-(new.total DIV minPoinPlg )
	WHERE idpelanggan=new.idpelanggan AND namapelanggan <> 'UMUM';
    END $$

DELIMITER ;

DELIMITER $$
-- 
-- Trigger  tg_batalretur
-- 

CREATE TRIGGER tg_batalretur AFTER DELETE ON detailreturjual FOR EACH ROW
BEGIN
	DECLARE anobuktijual VARCHAR (25) ;
  SELECT 
    nobuktijual INTO anobuktijual 
  FROM
    returpenjualan 
  WHERE nobukti = old.nobukti ;
  UPDATE 
    piutang 
  SET
    piutang.`piutang` = iF(old.gantiuang=True,piutang.`piutang` + old.subtotal,piutang.`piutang`),
    piutang.`kekurangan` = if(old.gantiuang=False,piutang.`kekurangan` + old.subtotal,piutang.`piutang`) 
  WHERE piutang.`fakturjual` = anobuktijual ;
  UPDATE 
    penjualandetil 
  SET
    jumlahretur = IF(old.gantiuang=TRUE,jumlahretur - old.jumlah,jumlahretur)
  WHERE fakturjual = anobuktijual 
    AND kodebarang = old.kodebarang ;
  UPDATE 
    penjualan  SET total = (SELECT SUM((harga-diskonrupiah) * (jumlah-jumlahretur)) FROM penjualandetil WHERE fakturjual=anobuktijual),
    grandtotal = total - diskon 
  WHERE fakturjual = anobuktijual ;
    END $$

DELIMITER ;

DELIMITER $$
-- 
-- Trigger  tg_poin
-- 

CREATE TRIGGER tg_poin AFTER INSERT ON penjualan FOR EACH ROW
BEGIN
	DECLARE minPoinPlg INT;
	
	SELECT minpoin INTO minPoinPlg FROM profile WHERE idprofil=1;
	
	
	UPDATE pelanggan SET poin=poin+(new.grandtotal DIV minPoinPlg)
	WHERE idpelanggan=new.idpelanggan AND namapelanggan <> 'UMUM';
    END $$

DELIMITER ;

/*!40014 SET FOREIGN_KEY_CHECKS=@OLD_FOREIGN_KEY_CHECKS */;
/*!40101 SET SQL_MODE=@OLD_SQL_MODE */;
