-- 
-- Table structure for table  pemakainbarang
-- 

DROP TABLE IF EXISTS pemakainbarang;
CREATE TABLE `pemakainbarang` (
  `idpemakain` int(11) NOT NULL AUTO_INCREMENT,
  `nobukti` varchar(50) DEFAULT NULL,
  `tgl` date DEFAULT NULL,
  `total` decimal(20,2) DEFAULT NULL,
  `iduser` int(11) DEFAULT NULL,
  `keterangan` varchar(200) DEFAULT NULL,
  PRIMARY KEY (`idpemakain`),
  UNIQUE KEY `nobukti` (`nobukti`)
) ENGINE=InnoDB DEFAULT CHARSET=latin1;

-- 
-- Dumping data for table pemakainbarang
-- 


-- 
-- Table structure for table  pembelian
-- 

DROP TABLE IF EXISTS pembelian;
CREATE TABLE `pembelian` (
  `idpembelian` int(11) NOT NULL AUTO_INCREMENT,
  `nobukti` varchar(50) DEFAULT NULL,
  `tgl` date DEFAULT NULL,
  `nopo` varchar(15) DEFAULT NULL,
  `refnota` varchar(30) DEFAULT NULL,
  `pembayaran` enum('CASH','KREDIT') DEFAULT NULL,
  `idsupplier` int(11) DEFAULT NULL,
  `iduser` int(11) DEFAULT NULL,
  `total` decimal(20,2) DEFAULT '0.00',
  `diskon` decimal(20,2) DEFAULT '0.00',
  `ppn` decimal(20,2) DEFAULT '0.00',
  `grandtotal` decimal(20,0) DEFAULT '0',
  `selesai` tinyint(1) DEFAULT '0',
  `returbeli` int(11) DEFAULT NULL,
  PRIMARY KEY (`idpembelian`),
  UNIQUE KEY `nobukti` (`nobukti`),
  KEY `idsupplier` (`idsupplier`),
  CONSTRAINT `pembelian_ibfk_1` FOREIGN KEY (`idsupplier`) REFERENCES `m_supplier` (`idsupplier`) ON DELETE CASCADE ON UPDATE CASCADE
) ENGINE=InnoDB AUTO_INCREMENT=35 DEFAULT CHARSET=latin1;

-- 
-- Dumping data for table pembelian
-- 

LOCK TABLES pembelian WRITE;
