-- 
-- Table structure for table  barangmodal
-- 

DROP TABLE IF EXISTS barangmodal;
CREATE TABLE `barangmodal` (
  `idbarangmodal` int(11) NOT NULL AUTO_INCREMENT,
  `nofaktur` varchar(25) DEFAULT NULL,
  `nokartu` varchar(25) DEFAULT NULL,
  `namabarang` varchar(150) DEFAULT NULL,
  `qty` decimal(20,2) DEFAULT NULL,
  `beli` decimal(20,2) DEFAULT NULL,
  `angkutan` decimal(20,2) DEFAULT NULL,
  `ppn` decimal(20,2) DEFAULT NULL,
  `ppnrp` decimal(20,2) DEFAULT NULL,
  `labapersen` decimal(20,2) DEFAULT NULL,
  `labarp` decimal(20,2) DEFAULT NULL,
  `modal` decimal(20,2) DEFAULT NULL,
  `hargaecer` decimal(20,2) DEFAULT NULL,
  `hargagrosir` decimal(20,2) DEFAULT NULL,
  `hargapartai` decimal(20,2) DEFAULT NULL,
  `hargapartai4` decimal(20,2) DEFAULT NULL,
  `hargapartai5` decimal(20,2) DEFAULT NULL,
  `kategori` varchar(20) DEFAULT NULL,
  PRIMARY KEY (`idbarangmodal`)
) ENGINE=InnoDB DEFAULT CHARSET=latin1;

-- 
-- Dumping data for table barangmodal
-- 


-- 
-- Table structure for table  barangrusak
-- 

DROP TABLE IF EXISTS barangrusak;
CREATE TABLE `barangrusak` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `nobukti` varchar(50) DEFAULT NULL,
  `tgl` date DEFAULT NULL,
  `total` decimal(20,2) DEFAULT NULL,
  `iduser` int(11) DEFAULT NULL,
  `keterangan` varchar(200) DEFAULT NULL,
  PRIMARY KEY (`id`),
  UNIQUE KEY `nobukti` (`nobukti`)
) ENGINE=InnoDB DEFAULT CHARSET=latin1;

-- 
-- Dumping data for table barangrusak
-- 


-- 
-- Table structure for table  bayarhutang
-- 

DROP TABLE IF EXISTS bayarhutang;
CREATE TABLE `bayarhutang` (
  `idbayarhutang` int(11) NOT NULL AUTO_INCREMENT,
  `nobukti` varchar(15) DEFAULT NULL,
  `tgl` date DEFAULT NULL,
  `idsupplier` int(11) DEFAULT NULL,
  `iduser` int(11) DEFAULT NULL,
  `keterangan` varchar(200) DEFAULT NULL,
  `nominal` decimal(20,2) DEFAULT NULL,
  `nobuktibeli` varchar(25) DEFAULT NULL,
  `nohutang` text,
  `diskon` decimal(20,2) DEFAULT NULL,
  `carabayar` varchar(25) DEFAULT NULL,
  PRIMARY KEY (`idbayarhutang`)
) ENGINE=InnoDB DEFAULT CHARSET=latin1;

