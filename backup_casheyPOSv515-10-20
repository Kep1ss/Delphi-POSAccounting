-- 
-- Table structure for table  penjualandetil
-- 

DROP TABLE IF EXISTS penjualandetil;
CREATE TABLE `penjualandetil` (
  `iddetil` int(11) NOT NULL AUTO_INCREMENT,
  `fakturjual` varchar(25) DEFAULT NULL,
  `kodebarang` varchar(15) DEFAULT NULL,
  `satuan` varchar(25) DEFAULT NULL,
  `harga` double(11,2) DEFAULT NULL,
  `hpp` double(11,2) DEFAULT NULL,
  `jumlah` double(11,2) DEFAULT NULL,
  `diskonpersen` decimal(20,2) DEFAULT NULL,
  `diskonrupiah` double(11,2) DEFAULT NULL,
  `diskon2` decimal(20,2) DEFAULT NULL,
  `diskon3` decimal(20,2) DEFAULT NULL,
  `subtotal` double(11,2) DEFAULT NULL,
  `isi` double(11,2) DEFAULT NULL,
  `lastupdate` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP,
  `lastuser` int(11) DEFAULT NULL,
  `profit` double(11,2) DEFAULT NULL,
  `idgudang` int(11) DEFAULT '1',
  `jumlahretur` decimal(20,2) DEFAULT '0.00',
  PRIMARY KEY (`iddetil`),
  KEY `fakturjual` (`fakturjual`),
  KEY `kodebarang` (`kodebarang`),
  KEY `idgudang` (`idgudang`),
  KEY `subtotal` (`subtotal`),
  CONSTRAINT `penjualandetil_ibfk_1` FOREIGN KEY (`fakturjual`) REFERENCES `penjualan` (`fakturjual`) ON DELETE CASCADE ON UPDATE CASCADE
) ENGINE=InnoDB DEFAULT CHARSET=latin1;

