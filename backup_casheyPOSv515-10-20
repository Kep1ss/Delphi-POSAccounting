DROP TABLE IF EXISTS pobeli;
CREATE TABLE `pobeli` (
  `idpobeli` int(11) NOT NULL AUTO_INCREMENT,
  `nobukti` varchar(15) DEFAULT NULL,
  `tgl` date DEFAULT NULL,
  `pembayaran` enum('CASH','KREDIT') DEFAULT NULL,
  `idsupplier` int(11) DEFAULT NULL,
  `total` decimal(20,2) DEFAULT NULL,
  `diskon` decimal(20,2) DEFAULT NULL,
  `ppn` decimal(20,2) DEFAULT NULL,
  `grandtotal` decimal(20,2) DEFAULT NULL,
  `createby` int(11) DEFAULT NULL,
  `keterangan` varchar(20) DEFAULT NULL,
  `terima` tinyint(1) DEFAULT '0',
  PRIMARY KEY (`idpobeli`),
  UNIQUE KEY `nobukti` (`nobukti`)
) ENGINE=InnoDB DEFAULT CHARSET=latin1;

