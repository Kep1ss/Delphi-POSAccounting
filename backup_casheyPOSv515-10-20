USE mysql;
DROP DATABASE IF EXISTS casheyv5db;
CREATE DATABASE casheyv5db;
USE casheyv5db;

-- 
-- StoredProc  fc_stok
-- 

DELIMITER $$
DROP FUNCTION IF EXISTS fc_stok $$
CREATE DEFINER=`root`@`localhost` FUNCTION `fc_stok`(iskodebarang Varchar (30),bulan int,tahun int) RETURNS decimal(20,2)
BEGIN
  declare isStok decimal (11, 2) ;
  select 
    SUM(saldoawal + masuk - keluar) into isStok 
  FROM kartustok WHERE kodebarang=iskodebarang and Month(tgl)=bulan and year(tgl)=tahun;
  
  return isStok;
  END $$
DELIMITER ;


-- 
-- StoredProc  fc_sumretur
-- 

DELIMITER $$
DROP FUNCTION IF EXISTS fc_sumretur $$
CREATE DEFINER=`root`@`localhost` FUNCTION `fc_sumretur`(xtgl date,member int) RETURNS decimal(20,2)
BEGIN
	declare totalretur decimal(20,2);
	
	select SUM(total) into totalretur from returpenjualan where tgl=xtgl and idpelanggan=member;
	
	return totalretur;
    END $$
DELIMITER ;


-- 
-- StoredProc  first_day
-- 

DELIMITER $$
DROP FUNCTION IF EXISTS first_day $$
CREATE DEFINER=`root`@`localhost` FUNCTION `first_day`(dt DATETIME) RETURNS date
BEGIN
  RETURN DATE_ADD(DATE_ADD(LAST_DAY(dt),INTERVAL 1 DAY),INTERVAL - 1 MONTH);
END $$
DELIMITER ;


-- 
-- StoredProc  sp_barangterjual
-- 

DELIMITER $$
DROP PROCEDURE IF EXISTS sp_barangterjual $$
CREATE DEFINER=`root`@`localhost` PROCEDURE `sp_barangterjual`(IN tgl1 DATE, IN tgl2 DATE,IN isCari TEXT)
BEGIN
	DECLARE str TEXT;
  DECLARE finish INT DEFAULT 0 ;
  DECLARE vIdBarang INT ;
  DECLARE vNamaBarang VARCHAR (250) ;
  DECLARE vJumlahTerjual INT ;
  DECLARE vHargaTerjual DECIMAL (20, 2) ;
  DECLARE vHargaRataRata DECIMAL (20, 2) ;
  DECLARE curBarang CURSOR FOR 
  SELECT 
    kodebarang,
    namabarang 
  FROM
    barang 
  ORDER BY kodebarang ASC ;
  DECLARE CONTINUE HANDLER FOR NOT FOUND SET finish = 1 ;
  DROP TABLE IF EXISTS tmp_rekap ;
  CREATE TEMPORARY TABLE tmp_rekap (
    id INT NOT NULL AUTO_INCREMENT,
    kodebarang VARCHAR(100),
    namabarang VARCHAR (200),
    jumlah INT (10),
    totalharga DECIMAL (20, 2),
    hargarata DECIMAL (20,2),
    PRIMARY KEY (id)
  ) ;
  
  INSERT INTO tmp_rekap (kodebarang, namabarang,jumlah,totalharga,hargarata) SELECT 
      b.kodebarang,
      b.namabarang,
      TRIM(IFNULL(SUM(d.jumlah - d.jumlahretur), 0)) + 0,
      TRIM(IFNULL(SUM((d.jumlah - d.jumlahretur)* (d.harga-d.diskonrupiah)), 0)) + 0,
      TRIM(
        IFNULL(
          SUM((d.jumlah - d.jumlahretur)* (d.harga-d.diskonrupiah)) / SUM(d.jumlah- d.jumlahretur),
          0
        )
      ) + 0
    FROM
      barang b 
      LEFT JOIN penjualandetil d 
        ON b.kodebarang = d.kodebarang 
      LEFT JOIN penjualan p 
        ON p.fakturjual = d.fakturjual 
    WHERE (
        p.tgljual >= tgl1
        AND p.tgljual <= tgl2
      ) 
      OR p.tgljual IS NULL 
    GROUP BY b.kodebarang ;
  
    
   SET str= ' SELECT t.kodebarang AS "Kode Barang",
	    t.namabarang AS "Nama Barang",
	    t.jumlah AS "Jumlah Terjual",
	    t.totalharga AS "Total Harga Terjual",
	    t.hargarata AS "Rata Rata Harga Terjual" FROM tmp_rekap t LEFT JOIN
	    barang b on b.kodebarang=t.kodebarang ';
   SET @str=CONCAT(str,' ',isCari);
   
   PREPARE stmt FROM @str ;
  EXECUTE stmt ;
  DEALLOCATE PREPARE stmt ;
  END $$
DELIMITER ;


-- 
-- StoredProc  sp_biaya
-- 

DELIMITER $$
DROP PROCEDURE IF EXISTS sp_biaya $$
CREATE DEFINER=`root`@`localhost` PROCEDURE `sp_biaya`(IN tgl1 DATE, IN tgl2 DATE)
BEGIN
  DECLARE finish INT DEFAULT 0 ;
  DECLARE cdate DATE ;
  DECLARE aAkun VARCHAR (10) ;
  DECLARE aNamaAkun VARCHAR (100) ;
  DECLARE aNominal DECIMAL (20, 2) ;
  DECLARE str TEXT ;
  DECLARE cur CURSOR FOR 
  SELECT 
    kodeakunperkiraan,
    namaakunperkiraan 
  FROM
    m_akunperkiraan 
  WHERE kodeakunperkiraan LIKE "502%" ;
  
  DECLARE CONTINUE HANDLER FOR NOT FOUND SET finish = 1;
  DROP TABLE IF EXISTS tmp_biaya ;
  CREATE TEMPORARY TABLE tmp_biaya (
    id INT NOT NULL AUTO_INCREMENT,
    kodeakunperkiraan VARCHAR (10),
    namaakunperkiraan VARCHAR (200),
    saldo DECIMAL (20, 2),
    PRIMARY KEY (id)
  ) ;
  
  OPEN cur ;
  curLoop :
  LOOP
	FETCH cur INTO aAkun,aNamaAkun;
	IF finish = 1 THEN LEAVE curLoop;
	END IF;
	SELECT 
      IFNULL(SUM(s_awal + debet - kredit), 0) INTO aNominal 
    FROM
      saldoakun 
    WHERE noakun = aAkun 
      AND tgl >= tgl1 
      AND tgl <= tgl2 ;
      
      INSERT INTO tmp_biaya (
      kodeakunperkiraan,
      namaakunperkiraan,
      saldo
    ) 
    VALUES
      (aAkun, aNamaAkun, aNominal) ;
  END LOOP ;
  CLOSE cur ;
  
  SELECT 
    * 
  FROM
    tmp_biaya ;
END $$
DELIMITER ;


-- 
-- StoredProc  sp_bukubesar
-- 

DELIMITER $$
DROP PROCEDURE IF EXISTS sp_bukubesar $$
CREATE DEFINER=`root`@`localhost` PROCEDURE `sp_bukubesar`(IN tgl1 DATE, IN tgl2 DATE,IN Akun VARCHAR(15))
BEGIN
     DECLARE aSaldo DECIMAL(20,2) DEFAULT 0;
  
     SET @aSaldo = 0;
     SELECT
           mb.jenisakun,                      
           sd.tgl,                  
           sd.noakun,               
           sd.nobukti,              
           mk.namaakunperkiraan,    
           sd.keterangan,           
           sd.s_awal,               
           sd.debet,                
           sd.kredit,                
           (CASE WHEN mb.jenisakun="A" THEN @aSaldo:=@aSaldo+(sd.s_awal+sd.debet-sd.kredit) ELSE
            @aSaldo:=@aSaldo+(sd.kredit-sd.debet+sd.s_awal) END ) AS s_akhir             
         FROM                       
         saldoakun sd             
           LEFT JOIN m_akunperkiraan 
           mk ON mk.`kodeakunperkiraan`=sd.`noakun` 
           LEFT JOIN m_subbukubesar sb ON mk.akunsubmaster=sb.akunsubmaster
           LEFT JOIN m_bukubesar mb ON sb.nomasterakun=mb.nomasterakun
           
           WHERE sd.tgl>=tgl1 AND sd.tgl<=tgl2 AND sd.noakun=akun  ORDER BY sd.tgl,sd.idsaldo ASC;
    END $$
DELIMITER ;


-- 
-- StoredProc  sp_fifo
-- 

DELIMITER $$
DROP PROCEDURE IF EXISTS sp_fifo $$
CREATE DEFINER=`root`@`localhost` PROCEDURE `sp_fifo`(
  in tgl1 date,
  in tgl2 date,
  in varKodeBarang varchar (50)
)
BEGIN
  SELECT 
    k.tgl,
    case
      when k.keterangan LIKE "PEMBELIAN%" 
      then "PEMBELIAN" 
      when k.keterangan LIKE "RETUR PENJUALAN%" 
      then "RETUR PENJUALAN" 
      when k.keterangan LIKE "PENJUALAN%" 
      then "PENJUALAN" 
      when k.keterangan LIKE "RETUR PEMBELIAN%" 
      then "RETUR PEMBELIAN" 
    end as "KETERANGAN",
    SUM(CASE WHEN (k.keterangan LIKE "PEMBELIAN%" or k.keterangan LIKE "RETUR PENJUALAN%") then k.masuk else 0 end) as "IN_QTY",
    k.hargabeli as "IN_HARGA",
    k.hargabeli * SUM(k.masuk) as "IN_TOTAL_HARGA_BELI",
    SUM(CASE WHEN (k.keterangan LIKE "PENJUALAN%" OR k.keterangan LIKE "RETUR PEMBELIAN%") THEN k.keluar ELSE 0 END) AS "OUT_QTY",
    k.hargabeli AS "IN_HARGA",
    k.hargabeli * SUM(k.keluar) AS "OUT_TOTAL_HARGA_BELI"
  from
    kartustok k 
  where k.tgl >= tgl1 
    and k.tgl <= tgl2 
    and k.kodebarang = varKodeBarang 
    AND (k.keterangan LIKE "PEMBELIAN%" 
    or k.keterangan LIKE "PENJUALAN%" 
    OR k.keterangan LIKE "RETUR PENJUALAN%" 
    or k.keterangan LIKE "RETUR PEMBELIAN%") GROUP BY k.tgl ;
END $$
DELIMITER ;


-- 
-- StoredProc  sp_hpp
-- 

DELIMITER $$
DROP PROCEDURE IF EXISTS sp_hpp $$
CREATE DEFINER=`root`@`localhost` PROCEDURE `sp_hpp`(IN tgl1 DATE, IN tgl2 DATE)
BEGIN
  DECLARE finish INT DEFAULT 0 ;
  DECLARE cdate DATE ;
  DECLARE aAkun VARCHAR (10) ;
  DECLARE aNamaAkun VARCHAR (100) ;
  DECLARE aNominal DECIMAL (20, 2) ;
  DECLARE str TEXT ;
  DECLARE cur CURSOR FOR 
  SELECT 
    kodeakunperkiraan,
    namaakunperkiraan 
  FROM
    m_akunperkiraan 
  WHERE kodeakunperkiraan LIKE "501%" ;
  
  DECLARE CONTINUE HANDLER FOR NOT FOUND SET finish = 1;
  DROP TABLE IF EXISTS tmp_hpp ;
  CREATE TEMPORARY TABLE tmp_hpp (
    id INT NOT NULL AUTO_INCREMENT,
    kodeakunperkiraan VARCHAR (10),
    namaakunperkiraan VARCHAR (200),
    saldo DECIMAL (20, 2),
    PRIMARY KEY (id)
  ) ;
  
  OPEN cur ;
  curLoop :
  LOOP
	FETCH cur INTO aAkun,aNamaAkun;
	IF finish = 1 THEN LEAVE curLoop;
	END IF;
	SELECT 
      IFNULL(SUM(s_awal + debet - kredit), 0) INTO aNominal 
    FROM
      saldoakun 
    WHERE noakun = aAkun 
      AND tgl >= tgl1 
      AND tgl <= tgl2 ;
      
      INSERT INTO tmp_hpp (
      kodeakunperkiraan,
      namaakunperkiraan,
      saldo
    ) 
    VALUES
      (aAkun, aNamaAkun, aNominal) ;
  END LOOP ;
  CLOSE cur ;
  
  SELECT 
    * 
  FROM
    tmp_hpp ;
END $$
DELIMITER ;


-- 
-- StoredProc  sp_kartustok
-- 

DELIMITER $$
DROP PROCEDURE IF EXISTS sp_kartustok $$
CREATE DEFINER=`root`@`localhost` PROCEDURE `sp_kartustok`(
  IN tgl1 DATE,
  IN tgl2 DATE,
  IN akodebarang VARCHAR (100),
  IN aGudang INT
)
BEGIN
  DECLARE aSaldo DECIMAL (20, 2) ;
  DECLARE aSaldoAwal DECIMAL (20, 2) ;
  IF DAY(tgl1) = 1 
  THEN 
  SELECT 
    saldoawal INTO aSaldoAwal 
  FROM
    kartustok 
  WHERE tgl = tgl1 
    AND kodebarang = aKodeBarang 
    AND idgudang = aGudang ORDER BY tgl ASC LIMIT 1;
  END IF;
  IF DAY(tgl1) > 1 
  THEN 
  SELECT 
    SUM(saldoawal + masuk - keluar) INTO aSaldoAwal 
  FROM
    kartustok 
  WHERE tgl >= first_day(tgl1) 
    AND tgl <= tgl1
    AND kodebarang = aKodeBarang 
    AND idgudang = aGudang ;
  END IF;
  
  IF aSaldoAwal IS NULL THEN
     SET @aSaldo = 0; 
  ELSE SET @aSaldo = aSaldoAwal;
  END IF;
  SELECT 
    k.tgl AS TANGGAL,
    b.kodebarang AS "KODE BARANG",
    b.namabarang AS "NAMA BARANG",
    aSaldoAwal AS "SALDO AWAL",
    k.masuk AS "MASUK",
    k.keluar AS "KELUAR",
    @aSaldo := (IFNULL(@aSaldo,0)  + k.masuk - k.keluar) AS "SALDO AKHIR",
    lk.namalokasi AS "GUDANG ",
    k.keterangan AS "KETERANGAN" 
  FROM
    kartustok k 
    LEFT JOIN barang b 
      ON b.`kodebarang` = k.`kodebarang` 
    LEFT JOIN lokasi lk 
      ON k.`idgudang` = lk.`idlokasi` 
  WHERE k.tgl >= tgl1 
    AND k.tgl <= tgl2 
    AND b.kodebarang = aKodebarang 
    AND k.idgudang = aGudang 
  ORDER BY k.tgl ASC ;
END $$
DELIMITER ;


-- 
-- StoredProc  sp_kartustok2
-- 

DELIMITER $$
DROP PROCEDURE IF EXISTS sp_kartustok2 $$
CREATE DEFINER=`root`@`localhost` PROCEDURE `sp_kartustok2`(IN tgl1 DATE, IN tgl2 DATE,isCari VARCHAR(200))
BEGIN
  DECLARE finish INTEGER DEFAULT 0 ;
  DECLARE str VARCHAR (1000) ;
  DECLARE strCheck VARCHAR (5);
  DECLARE GudangName VARCHAR (50) ;
  DECLARE aSaldo DECIMAL(20,2);
  DECLARE aGudang CURSOR FOR 
  SELECT DISTINCT 
    namalokasi 
  FROM
    lokasi ;
  DECLARE CONTINUE HANDLER FOR NOT FOUND SET finish = 1 ;
  SET Str = 'SELECT b.kodebarang as "Kode Barang",b.namabarang as "Nama Barang",ks.tgl as "Tanggal",SUM(ks.saldoawal) as "SALDO_AWAL",' ;
  SET aSaldo = 0;
  OPEN aGudang ;
  Loop_Gudang :
  LOOP
    FETCH aGudang INTO GudangName ;
    IF Finish = 1 
    THEN LEAVE Loop_Gudang ;
    END IF ;
    
    SET str = CONCAT(
      str,'
      SUM(CASE WHEN l.namalokasi="',
      GudangName,
      '" THEN ks.masuk  ElSE 0 END) AS "',GudangName,'_MASUK" ,
      SUM(CASE WHEN l.namalokasi="',
      GudangName,
      '" THEN ks.keluar  ElSE 0 END) AS "',GudangName,'_KELUAR",') ;
  END LOOP ;
  CLOSE aGudang ;
  SET str = SUBSTR(str, 1, CHAR_LENGTH(str) - 1) ;
  
  SET @str = CONCAT(
    str,
    '  FROM barang b LEFT JOIN kategori k ON k.idkategori=b.idkategori
				   RIGHT JOIN kartustok ks 
				   ON ks.kodebarang=b.kodebarang LEFT JOIN lokasi l ON l.idlokasi=ks.idgudang 
				   WHERE ks.tgl>="',
    tgl1,
    '" AND ks.tgl<="',
    tgl2,
    '" ',isCari,' GROUP BY ks.kodebarang,ks.tgl ORDER BY ks.tgl ASC ;') ;
  
 
  
  PREPARE stmt FROM @str ;
  EXECUTE stmt ;
  DEALLOCATE PREPARE stmt ;
END $$
DELIMITER ;


-- 
-- StoredProc  sp_kartustokonline
-- 

DELIMITER $$
DROP PROCEDURE IF EXISTS sp_kartustokonline $$
CREATE DEFINER=`root`@`localhost` PROCEDURE `sp_kartustokonline`(
	  IN tgl1 DATE,
	  IN tgl2 DATE,
	  IN akodebarang VARCHAR (100),
	  IN aGudang INT)
BEGIN
	DECLARE aSaldo DECIMAL (20, 2) ;
  DECLARE aSaldoAwal DECIMAL (20, 2) ;
  IF DAY(tgl1) = 1 
  THEN 
  SELECT 
    saldoawal INTO aSaldoAwal 
  FROM
    kartustok 
  WHERE tgl = tgl1 
    AND kodebarang = aKodeBarang 
    AND idgudang = aGudang ORDER BY tgl ASC LIMIT 1;
  END IF;
  IF DAY(tgl1) > 1 
  THEN 
  SELECT 
    SUM(saldoawal + masuk - keluar) INTO aSaldoAwal 
  FROM
    kartustok 
  WHERE tgl >= first_day(tgl1) 
    AND tgl <= tgl1
    AND kodebarang = aKodeBarang 
    AND idgudang = aGudang ;
  END IF;
  
  IF aSaldoAwal IS NULL THEN
     SET @aSaldo = 0; 
  ELSE SET @aSaldo = aSaldoAwal;
  END IF;
  SELECT 
    k.tgl,
    b.kodebarang,
    b.namabarang,
    TRIM(aSaldoAwal)+0 AS saldoawal,
    k.masuk,
    k.keluar,
    @aSaldo := (IFNULL(@aSaldo,0)  + k.masuk - k.keluar) AS "saldo",
    lk.namalokasi AS "gudang",
    k.keterangan AS "keterangan" 
  FROM
    kartustok k 
    LEFT JOIN barang b 
      ON b.`kodebarang` = k.`kodebarang` 
    LEFT JOIN lokasi lk 
      ON k.`idgudang` = lk.`idlokasi` 
  WHERE k.tgl >= tgl1 
    AND k.tgl <= tgl2 
    AND b.kodebarang = aKodebarang 
    AND k.idgudang = aGudang 
  ORDER BY k.tgl ASC ;
    END $$
DELIMITER ;


-- 
-- StoredProc  sp_labarugi
-- 

DELIMITER $$
DROP PROCEDURE IF EXISTS sp_labarugi $$
CREATE DEFINER=`root`@`localhost` PROCEDURE `sp_labarugi`(IN tgl1 DATE, IN tgl2 DATE)
BEGIN
  
  SELECT 
  
    (
      CASE
        WHEN bb.jenisakun = "A" 
        THEN SUM(s.s_awal) + SUM(s.debet) - SUM(s.kredit) 
        ELSE SUM(s.s_awal) + SUM(s.kredit) - SUM(s.debet) 
      END
    ) AS saldoakhir,
    SUM(
      CASE
        WHEN bb.jenisakun = "A" 
        THEN s.s_awal+s.debet- s.kredit
      END
    ) AS "Biaya",
    SUM(
      CASE
        WHEN bb.jenisakun = "P" 
        THEN s.s_awal+ s.kredit - s.debet
      END
    ) AS "Pendapatan" 
  FROM
    m_akunperkiraan ak 
    LEFT JOIN saldoakun s 
      ON s.`noakun` = ak.`kodeakunperkiraan` 
    LEFT JOIN m_subbukubesar ms 
      ON ms.akunsubmaster = ak.`akunsubmaster` 
    LEFT JOIN m_bukubesar bb 
      ON bb.`nomasterakun` = ms.`nomasterakun` 
  WHERE s.tgl >= tgl1 
    AND s.tgl <= tgl2 
    AND bb.nomasterakun IN (4, 5);
 END $$
DELIMITER ;


-- 
-- StoredProc  sp_lapbulan
-- 

DELIMITER $$
DROP PROCEDURE IF EXISTS sp_lapbulan $$
CREATE DEFINER=`root`@`localhost` PROCEDURE `sp_lapbulan`(in TGL1 date,IN TGL2 DATE)
BEGIN
  DECLARE finish INT DEFAULT 0 ;
  DECLARE cdate DATE ;
  declare strCheck varchar (5);
  DECLARE str VARCHAR (10000) DEFAULT 'select DATE_FORMAT(p.tgljual,"%b") as "BULAN",pl.namapelanggan as "PELANGGAN",s.namasales as "SALES",' ;
  DECLARE curs CURSOR FOR 
  SELECT 
   DISTINCT tgljual 
  FROM
    penjualan 
  WHERE tgljual>= TGL1
    AND tgljual<= TGL2 ;
    
  DECLARE CONTINUE HANDLER FOR NOT FOUND SET finish = 1 ;
  OPEN curs ;
  my_loop :
  LOOP
    FETCH curs INTO cdate ;
    IF finish = 1 
    THEN LEAVE my_loop ;
    END IF ;
    SET str = CONCAT(
      str,
      ' SUM(case when p.tgljual ="',
      date(cdate),
      '" then p.grandtotal else 0 end) as "',
      date_FORMAT(cdate,"%d %b %y"),
      '",'
    ) ;
  END LOOP ;
  CLOSE curs ;
  SET str = SUBSTR(str, 1, CHAR_LENGTH(str) - 1) ;
  
  SET strCheck = right(str,1);
  
  if strCheck <>',' then
    SET str =CONCAT(str,',');
  end if;
  SET @str = CONCAT(
    str,
    ' SUM(Grandtotal) as TOTAL from penjualan p 
    LEFT JOIN pelanggan pl ON pl.idpelanggan=p.idpelanggan 
    LEFT JOIN sales s ON s.idsales=pl.idsales where p.tgljual>="',
    TGL1,
    '" and p.tgljual<="',
    TGL2,
    '" GROUP BY pl.idpelanggan'
  ) ;
  PREPARE stmt FROM @str ;
  EXECUTE stmt ;
  DEALLOCATE PREPARE stmt ;
END $$
DELIMITER ;


-- 
-- StoredProc  sp_lapbulanPembelian
-- 

DELIMITER $$
DROP PROCEDURE IF EXISTS sp_lapbulanPembelian $$
CREATE DEFINER=`root`@`localhost` PROCEDURE `sp_lapbulanPembelian`(in TGL1 date,IN TGL2 DATE)
BEGIN
  DECLARE finish INT DEFAULT 0 ;
  DECLARE cdate DATE ;
  declare strCheck varchar (5);
  DECLARE str VARCHAR (10000) DEFAULT 'select DATE_FORMAT(p.tgl,"%b") as "BULAN",s.namasupplier as "SUPPLIER",' ;
  DECLARE curs CURSOR FOR 
  SELECT 
   DISTINCT tgl 
  FROM
    pembelian 
  WHERE tgl>= TGL1
    AND tgl<= TGL2 ;
    
  DECLARE CONTINUE HANDLER FOR NOT FOUND SET finish = 1 ;
  OPEN curs ;
  my_loop :
  LOOP
    FETCH curs INTO cdate ;
    IF finish = 1 
    THEN LEAVE my_loop ;
    END IF ;
    SET str = CONCAT(
      str,
      ' SUM(case when p.tgl ="',
      date(cdate),
      '" then p.total else 0 end) as "',
      date_FORMAT(cdate,"%d %b %y"),
      '",'
    ) ;
  END LOOP ;
  CLOSE curs ;
  SET str = SUBSTR(str, 1, CHAR_LENGTH(str) - 1) ;
  
  SET strCheck = right(str,1);
  
  if strCheck <>',' then
    SET str =CONCAT(str,',');
  end if;
  SET @str = CONCAT(
    str,
    ' SUM(p.total) as TOTAL from pembelian p 
    LEFT JOIN m_supplier s ON s.idsupplier=p.idsupplier
    where p.tgl>="',
    TGL1,
    '" and p.tgl<="',
    TGL2,
    '" GROUP BY s.idsupplier ORDER BY s.namasupplier ASC'
  ) ;
  PREPARE stmt FROM @str ;
  EXECUTE stmt ;
  DEALLOCATE PREPARE stmt ;
END $$
DELIMITER ;


-- 
-- StoredProc  sp_lapbulanx
-- 

DELIMITER $$
DROP PROCEDURE IF EXISTS sp_lapbulanx $$
CREATE DEFINER=`root`@`localhost` PROCEDURE `sp_lapbulanx`(in TGL1 date,IN TGL2 DATE)
BEGIN
  DECLARE finish INT DEFAULT 0 ;
  DECLARE cdate DATE ;
  declare strCheck varchar (5);
  DECLARE str VARCHAR (10000) DEFAULT '' ;
  DECLARE curs CURSOR FOR 
  SELECT 
   DISTINCT tgljual 
  FROM
    penjualan 
  WHERE tgljual>= TGL1
    AND tgljual<= TGL2 ;
    
  DECLARE CONTINUE HANDLER FOR NOT FOUND SET finish = 1 ;
  
  SET str = CONCAT('select "',DATE_FORMAT(TGL2,'%b'),'" as "BULAN",pl.namapelanggan as "PELANGGAN",s.namasales as "SALES",');
  OPEN curs ;
  my_loop :
  LOOP
    FETCH curs INTO cdate ;
    IF finish = 1 
    THEN LEAVE my_loop ;
    END IF ;
    SET str = CONCAT(
      str,
      ' SUM(case when p.tgl ="',
      date(cdate),
      '" then IFNULL(p.totalpenjualan,0)- IFNULL(p.totalreturpenjualan,0) else 0 end) as "',
      date_FORMAT(cdate,"%d %b %y"),
      '",'
    ) ;
  END LOOP ;
  CLOSE curs ;
  SET str = SUBSTR(str, 1, CHAR_LENGTH(str) - 1) ;
  
  SET strCheck = right(str,1);
  
  if strCheck <>',' then
    SET str =CONCAT(str,',');
  end if;
  SET @str = CONCAT(
    str,
    ' SUM(IFNULL(p.totalpenjualan,0)-IFNULL(p.totalreturpenjualan,0)) as TOTAL from pelanggan pl LEFT JOIN rekapjualretur p 
    ON pl.idpelanggan=p.idpelanggan 
    LEFT JOIN sales s ON s.idsales=pl.idsales GROUP BY pl.idpelanggan'
  ) ;
  PREPARE stmt FROM @str ;
  EXECUTE stmt ;
  DEALLOCATE PREPARE stmt ;
END $$
DELIMITER ;


-- 
-- StoredProc  sp_laptahun
-- 

DELIMITER $$
DROP PROCEDURE IF EXISTS sp_laptahun $$
CREATE DEFINER=`root`@`localhost` PROCEDURE `sp_laptahun`(in thn Varchar(6))
BEGIN
  DECLARE finish INT DEFAULT 0 ;
  DECLARE cdate varchar(20) ;
  declare strCheck varchar (5);
  DECLARE str VARCHAR (10000) DEFAULT 'select YEAR(p.tgljual) as "TAHUN",pl.namapelanggan as "PELANGGAN",s.namasales as "SALES",' ;
  DECLARE curs CURSOR FOR 
  SELECT 
   DISTINCT DATE_FORMAT(tgljual,'%b') 
  FROM
    penjualan 
  WHERE YEAR(tgljual)= thn;
    
  DECLARE CONTINUE HANDLER FOR NOT FOUND SET finish = 1 ;
  OPEN curs ;
  my_loop :
  LOOP
    FETCH curs INTO cdate ;
    IF finish = 1 
    THEN LEAVE my_loop ;
    END IF ;
    SET str = CONCAT(
      str,
      ' SUM(case when DATE_FORMAT(p.tgljual,"%b")="',
      cdate,
      '" then p.grandtotal else 0 end) as "',
      cdate,
      '",'
    ) ;
  END LOOP ;
  CLOSE curs ;
  SET str = SUBSTR(str, 1, CHAR_LENGTH(str) - 1) ;
  
  SET strCheck = right(str,1);
  
  if strCheck <>',' then
    SET str =CONCAT(str,',');
  end if;
  SET @str = CONCAT(
    str,
    ' SUM(Grandtotal) as TOTAL from penjualan p 
    LEFT JOIN pelanggan pl ON pl.idpelanggan=p.idpelanggan 
    LEFT JOIN sales s ON s.idsales=pl.idsales where YEAR(p.tgljual)="',thn,'" GROUP BY pl.idpelanggan'
  ) ;
  PREPARE stmt FROM @str ;
  EXECUTE stmt ;
  DEALLOCATE PREPARE stmt ;
END $$
DELIMITER ;


-- 
-- StoredProc  sp_lapTahunx
-- 

DELIMITER $$
DROP PROCEDURE IF EXISTS sp_lapTahunx $$
CREATE DEFINER=`root`@`localhost` PROCEDURE `sp_lapTahunx`(in thn varchar(6))
BEGIN
  DECLARE finish INT DEFAULT 0 ;
  DECLARE cdate varchar(20) ;
  declare strCheck varchar (5);
  DECLARE str VARCHAR (10000) DEFAULT '' ;
  DECLARE curs CURSOR FOR 
  SELECT 
   DISTINCT DATE_FORMAT(tgl,'%b') 
  FROM
    rekapjualretur 
  WHERE YEAR(tgl)= thn;  
  DECLARE CONTINUE HANDLER FOR NOT FOUND SET finish = 1 ;
  
  SET str = CONCAT('select ',thn,' as "TAHUN",pl.namapelanggan as "PELANGGAN",s.namasales as "SALES", ') ; 
  
  OPEN curs ;
  my_loop :
  LOOP
    FETCH curs INTO cdate ;
    IF finish = 1 
    THEN LEAVE my_loop ;
    END IF ;
    SET str = CONCAT(
      str,
      ' SUM(case when DATE_FORMAT(p.tgl,"%b")="',
      cdate,
      '" then IFNULL(p.totalpenjualan,0)-IFNULL(p.totalreturpenjualan,0) else 0 end) as "',
      cdate,
      '",'
    ) ;
  END LOOP ;
  CLOSE curs ;
  SET str = SUBSTR(str, 1, CHAR_LENGTH(str) - 1) ;
  
  SET strCheck = right(str,1);
  
  if strCheck <>',' then
    SET str =CONCAT(str,',');
  end if;
  SET @str = CONCAT(
    str,
    ' SUM(IFNULL(p.totalpenjualan,0)-IFNULL(p.totalreturpenjualan,0)) as TOTAL from pelanggan pl LEFT JOIN rekapjualretur p 
    ON pl.idpelanggan=p.idpelanggan 
    LEFT JOIN sales s ON s.idsales=pl.idsales where YEAR(p.tgl)="',thn,'" OR p.idpelanggan is null GROUP BY pl.idpelanggan'
  ) ;
  PREPARE stmt FROM @str ;
  EXECUTE stmt ;
  DEALLOCATE PREPARE stmt ;
END $$
DELIMITER ;


-- 
-- StoredProc  sp_NominalPembelianBarangBulan
-- 

DELIMITER $$
DROP PROCEDURE IF EXISTS sp_NominalPembelianBarangBulan $$
CREATE DEFINER=`root`@`localhost` PROCEDURE `sp_NominalPembelianBarangBulan`(IN thn VARCHAR(6))
BEGIN
  DECLARE finish INT DEFAULT 0 ;
  DECLARE cdate VARCHAR(20) ;
  DECLARE strCheck VARCHAR (5);
  DECLARE str VARCHAR (10000) DEFAULT '' ;
  DECLARE curs CURSOR FOR 
  SELECT 
   DISTINCT DATE_FORMAT(tgl,'%b') 
  FROM
    pembelian 
  WHERE YEAR(tgl)>= thn ;
    
  DECLARE CONTINUE HANDLER FOR NOT FOUND SET finish = 1 ;
  
  SET str = CONCAT('select "',thn,'" as "TAHUN",s.namasupplier as 
  "SUPPLIER",b.namabarang as "NAMA BARANG",');
  OPEN curs ;
  my_loop :
  LOOP
    FETCH curs INTO cdate ;
    IF finish = 1 
    THEN LEAVE my_loop ;
    END IF ;
    SET str = CONCAT(
      str,
      ' SUM(case when DATE_FORMAT(p.tgl,"%b") ="',
      cdate,
      '" then IFNULL(pd.subtotal,0) else 0 end) as "',
      cdate,
      '",'
    ) ;
  END LOOP ;
  CLOSE curs ;
  SET str = SUBSTR(str, 1, CHAR_LENGTH(str) - 1) ;
  
  SET strCheck = RIGHT(str,1);
  
  IF strCheck <>',' THEN
    SET str =CONCAT(str,',');
  END IF;
  SET @str = CONCAT(
    str,
    ' SUM(IFNULL(pd.subtotal,0)) as TOTAL from m_supplier s LEFT JOIN pembelian p 
    ON s.idsupplier=p.idsupplier  
    LEFT JOIN detailbeli pd ON pd.nobukti=p.nobukti
    LEFT JOIN barang b ON b.kodebarang=pd.kodebarang
    WHERE YEAR(p.tgl)="',thn,'" OR p.idsupplier is null
    GROUP BY pd.kodebarang,p.idsupplier'
  ) ;
  PREPARE stmt FROM @str ;
  EXECUTE stmt ;
  DEALLOCATE PREPARE stmt ;
END $$
DELIMITER ;


-- 
-- StoredProc  sp_NominalPembelianBarangHari
-- 

DELIMITER $$
DROP PROCEDURE IF EXISTS sp_NominalPembelianBarangHari $$
CREATE DEFINER=`root`@`localhost` PROCEDURE `sp_NominalPembelianBarangHari`(IN TGL1 DATE,IN TGL2 DATE)
BEGIN
  DECLARE finish INT DEFAULT 0 ;
  DECLARE cdate DATE ;
  DECLARE strCheck VARCHAR (5);
  DECLARE str VARCHAR (10000) DEFAULT '' ;
  DECLARE curs CURSOR FOR 
  SELECT 
   DISTINCT tgl
  FROM
    pembelian 
  WHERE tgl>= TGL1
    AND tgl<= TGL2 ;
    
  DECLARE CONTINUE HANDLER FOR NOT FOUND SET finish = 1 ;
  
  SET str = CONCAT('select "',DATE_FORMAT(TGL2,'%b'),'" as "BULAN",s.namasupplier as 
  "SUPPLIER",b.namabarang as "NAMA BARANG",');
  OPEN curs ;
  my_loop :
  LOOP
    FETCH curs INTO cdate ;
    IF finish = 1 
    THEN LEAVE my_loop ;
    END IF ;
    SET str = CONCAT(
      str,
      ' SUM(case when p.tgl ="',
      DATE(cdate),
      '" then IFNULL(pd.subtotal,0) else 0 end) as "',
      DATE_FORMAT(cdate,"%d %b %y"),
      '",'
    ) ;
  END LOOP ;
  CLOSE curs ;
  SET str = SUBSTR(str, 1, CHAR_LENGTH(str) - 1) ;
  
  SET strCheck = RIGHT(str,1);
  
  IF strCheck <>',' THEN
    SET str =CONCAT(str,',');
  END IF;
  SET @str = CONCAT(
    str,
    ' SUM(IFNULL(pd.subtotal,0)) as TOTAL from m_supplier s LEFT JOIN pembelian p 
    ON s.idsupplier=p.idsupplier
    LEFT JOIN detailbeli  pd ON pd.nobukti=p.nobukti
    LEFT JOIN barang b ON b.kodebarang=pd.kodebarang
    WHERE (p.idsupplier IS NULL OR (p.tgl>="',tgl1,'" AND p.tgl<="',tgl2,'"))
    GROUP BY pd.kodebarang,p.idsupplier'
  ) ;
  PREPARE stmt FROM @str ;
  EXECUTE stmt ;
  DEALLOCATE PREPARE stmt ;
END $$
DELIMITER ;


-- 
-- StoredProc  sp_pendapatan
-- 

DELIMITER $$
DROP PROCEDURE IF EXISTS sp_pendapatan $$
CREATE DEFINER=`root`@`localhost` PROCEDURE `sp_pendapatan`(IN tgl1 DATE, IN tgl2 DATE)
BEGIN
  DECLARE finish INT DEFAULT 0 ;
  DECLARE cdate DATE ;
  DECLARE aAkun VARCHAR (10) ;
  DECLARE aNamaAkun VARCHAR (100) ;
  DECLARE aNominal DECIMAL (20, 2) ;
  DECLARE str TEXT ;
  DECLARE cur CURSOR FOR 
  SELECT 
    kodeakunperkiraan,
    namaakunperkiraan 
  FROM
    m_akunperkiraan 
  WHERE kodeakunperkiraan LIKE "40%" ;
  
  DECLARE CONTINUE HANDLER FOR NOT FOUND SET finish = 1;
  DROP TABLE IF EXISTS tmp_pendapatan ;
  CREATE TEMPORARY TABLE tmp_pendapatan (
    id INT NOT NULL AUTO_INCREMENT,
    kodeakunperkiraan VARCHAR (10),
    namaakunperkiraan VARCHAR (200),
    saldo DECIMAL (20, 2),
    PRIMARY KEY (id)
  ) ;
  
  OPEN cur ;
  curLoop :
  LOOP
	FETCH cur INTO aAkun,aNamaAkun;
	IF finish = 1 THEN LEAVE curLoop;
	END IF;
	SELECT 
      IFNULL(SUM(s_awal+kredit-debet), 0) INTO aNominal 
    FROM
      saldoakun 
    WHERE noakun = aAkun 
      AND tgl >= tgl1 
      AND tgl <= tgl2 ;
      
      INSERT INTO tmp_pendapatan (
      kodeakunperkiraan,
      namaakunperkiraan,
      saldo
    ) 
    VALUES
      (aAkun, aNamaAkun, aNominal) ;
  END LOOP ;
  CLOSE cur ;
  
  SELECT 
    * 
  FROM
    tmp_pendapatan ;
END $$
DELIMITER ;


-- 
-- StoredProc  sp_piutang
-- 

DELIMITER $$
DROP PROCEDURE IF EXISTS sp_piutang $$
CREATE DEFINER=`root`@`localhost` PROCEDURE `sp_piutang`(IN isCari VARCHAR (100))
BEGIN
  DECLARE str TEXT ;
  
  SET str = 'SELECT pl.idpelanggan,pl.kodepelanggan,pl.namapelanggan,SUM(p.piutang) as totalpiutang,
            SUM(p.dibayar) as dibayar,SUM(p.kekurangan) as kekurangan,pl.top FROM piutang p 
            LEFT JOIN penjualan pj ON pj.fakturjual=p.fakturjual lEFT JOIN pelanggan pl ON pj.idpelanggan=pl.idpelanggan WHERE 
            p.lunas=FALSE ';
  SET @str = CONCAT(str,isCari,' GROUP BY pl.idpelanggan ');
  PREPARE stmt FROM @str ;
  EXECUTE stmt ;
  DEALLOCATE PREPARE stmt ;
  
END $$
DELIMITER ;


-- 
-- StoredProc  sp_PivotBarangBulan
-- 

DELIMITER $$
DROP PROCEDURE IF EXISTS sp_PivotBarangBulan $$
CREATE DEFINER=`root`@`localhost` PROCEDURE `sp_PivotBarangBulan`(IN thn VARCHAR(6))
BEGIN
  DECLARE finish INT DEFAULT 0 ;
  DECLARE cdate VARCHAR(20) ;
  DECLARE strCheck VARCHAR (5);
  DECLARE str VARCHAR (10000) DEFAULT '' ;
  DECLARE curs CURSOR FOR 
  SELECT 
   DISTINCT DATE_FORMAT(tgljual,'%b') 
  FROM
    penjualan 
  WHERE YEAR(tgljual)>= thn ;
    
  DECLARE CONTINUE HANDLER FOR NOT FOUND SET finish = 1 ;
  
  SET str = CONCAT('select "',thn,'" as "TAHUN",pl.namapelanggan as 
  "PELANGGAN",s.namasales as "SALES",b.namabarang as "NAMA BARANG",');
  OPEN curs ;
  my_loop :
  LOOP
    FETCH curs INTO cdate ;
    IF finish = 1 
    THEN LEAVE my_loop ;
    END IF ;
    SET str = CONCAT(
      str,
      ' SUM(case when DATE_FORMAT(p.tgljual,"%b") ="',
      cdate,
      '" then IFNULL(pd.jumlah,0) else 0 end) as "',
      cdate,
      '",'
    ) ;
  END LOOP ;
  CLOSE curs ;
  SET str = SUBSTR(str, 1, CHAR_LENGTH(str) - 1) ;
  
  SET strCheck = RIGHT(str,1);
  
  IF strCheck <>',' THEN
    SET str =CONCAT(str,',');
  END IF;
  SET @str = CONCAT(
    str,
    ' SUM(IFNULL(pd.jumlah,0)) as TOTAL from pelanggan pl LEFT JOIN penjualan p 
    ON pl.idpelanggan=p.idpelanggan 
    LEFT JOIN sales s ON s.idsales=pl.idsales 
    LEFT JOIN penjualandetil pd ON pd.fakturjual=p.fakturjual
    LEFT JOIN barang b ON b.kodebarang=pd.kodebarang WHERE YEAR(p.tgljual)="',thn,'" OR p.idpelanggan is null
    GROUP BY pd.kodebarang,p.idpelanggan'
  ) ;
  PREPARE stmt FROM @str ;
  EXECUTE stmt ;
  DEALLOCATE PREPARE stmt ;
END $$
DELIMITER ;


